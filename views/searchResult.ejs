<%- include('layout/studentHeader') %>
<div class="container">
<div class="row">
<div class="col-lg-6 mx-auto mt-4">
    <div class="card shadow ">
        <div class="card-header bg-dark text-center">
          <h4 class="text-light">Search Result</h4>
        </div>
        <div class="card-body p-4">
            <form action="/searchResult" method="post" id="login-form">
                <div class="mb-3">
                    <label for="email">Name </label>
                    <input type="name" name="name" id="" class="form-control form-control-lg" 
                    placeholder="Enter Name" required>
                </div>
                <div class="mb-3">
                    <label for="rollnumber">Roll Number</label>
                    <input type="number" name="rollnumber" id="" class="form-control form-control-lg" 
                    required>
                </div>
                <div class="mb-3 d-grid">
                    <input type="submit" value="Search" name="submit" class="btn btn-primary btn-lg">
                 </div>
            </form>
        </div>
      </div>
</div>
</div>
</div>
<%- include('layout/footer') %>  